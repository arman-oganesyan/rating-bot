{
    "app": {
        "token": "<BOT TOKEN>",
        "vote_timeout": 0,
        "stat_timeout": 10800
    },
    "logger": {
        "categories": {
            "default": {
                "appenders": [
                    "all"
                ],
                "level": "trace"
            }
        },
        "appenders": {
            "all": {
                "type": "fileSync",
                "maxLogSize": 52428800,
                "filename": "/Users/armanoganesyan/Projects/rating-bot/logs/all.log",
                "layout": {
                    "type": "basic"
                }
            }
        }
    },
    "mongo": {
        "url": "<MONGO URL>",
        "collection": "DEV_rating",
        "options": {
            "useUnifiedTopology": true
        }
    },
    "tg": {
        "polling": {
            "autoStart": false
        }
    },
    "redis": {
        "socket": {
            "host": "<REDIS IP>",
            "port": 6379
        }
    }
}